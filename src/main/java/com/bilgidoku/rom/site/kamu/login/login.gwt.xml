<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.6.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='login'>

	<inherits name="com.google.gwt.logging.Logging"/>

 	<set-property name="gwt.logging.enabled" value="TRUE" />
    <set-property name="gwt.logging.logLevel" value="INFO" />
    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />
    <set-property name="gwt.logging.developmentModeHandler"
        value="ENABLED" />
    <set-property name="gwt.logging.hasWidgetsHandler" value="DISABLED" />
    <set-property name="gwt.logging.simpleRemoteHandler"
        value="DISABLED" />

	<inherits name="com.google.gwt.user.User" />
<!-- 	<inherits name='com.google.gwt.user.theme.chrome.Chrome' /> -->
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='com.google.gwt.json.JSON' />



	<inherits name='rom-min' />
	<inherits name='rom-gwt-client-common' />
	<inherits name='rom-gwt-client-util' />
	<inherits name='rom-gwt-shared' />
	<inherits name='rom-shared' />

	<inherits name='rom-gwt-client-rom' />
	<inherits name='rom-gwt-client-site' />

	<inherits name='rom-gwt-client-tepeweb' />
	<inherits name='rom-gwt-client-service' />

	<source path="client" />

	<entry-point class="com.bilgidoku.rom.site.kamu.login.client.login"></entry-point>
	<!-- <set-property name="user.agent" value="gecko1_8" /> -->
	<set-property name="user.agent" value="safari" /> 

</module>
